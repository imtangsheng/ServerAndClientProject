                             <string notr="true">/* 整体滚动条 */
QScrollBar:horizontal {
    height: 16px;                /* 滚动条高度 */
	border: none;
    background: transparent;      /* 滚动条背景设为透明 */
    margin: 0px 36px 0px 36px;   /* 留出左右按钮的位置 */
}

/* 滚动条滑块 */
QScrollBar::handle:horizontal {
	background-image: url(:/assets/TaskHub/circle.png);
    min-width: 32px;             /* 滑块最小宽度 */	
}
/* 左箭头按钮 */
QScrollBar::sub-line:horizontal {
    width: 20px;                 /* 按钮宽度 */
    background: transparent;
    subcontrol-position: left;   /* 左侧位置 */
    subcontrol-origin: margin;   /* 控件位置基于margin */
    position: absolute;
    left: 0px;
}
/* 左箭头图标 */
QScrollBar::left-arrow:horizontal {
	image: url(:/assets/TaskHub/subtraction.png);
	width:16px;
	height:16px;
}
/* 右箭头按钮 */
QScrollBar::add-line:horizontal {
    width: 20px;                 /* 按钮宽度 */
    background: transparent;
    subcontrol-position: right;  /* 右侧位置 */
    subcontrol-origin: margin;   /* 控件位置基于margin */
    position: absolute;
    right: 0px;
}

/* 右箭头图标 */
