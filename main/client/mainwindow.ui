/* 基础按钮样式 */
QRadioButton {
    min-width: 115px;
    min-height: 107px;
	font-size: 24px;   
	/*没有 text-align 属性,没有文字居中*/
	spacing: 12px; /* 减少文本和指示器的间距 */
}

/* 完全移除所有状态下的指示器 */
QRadioButton::indicator {
    width: 7px;
    height: 63px;
    border: none;
    background: none;
    margin: 0px;
    padding: 0px;
}
/* 确保伪状态也不会显示任何内容 */
QRadioButton::indicator:unchecked{
    image: none;
    border: none;
    background: none;
}

QRadioButton::indicator:checked{
    width: 7px;
    height: 63px;
	image: url(:/assets/images/Button/Rectangle7x63.png);
}

/* 选中状态样式 */
QRadioButton:checked {
	/*方法二:使用背景图片
	image: url(:/assets/images/Button/Rectangle7x63.png);
	image-position: left center;*/
    color: qlineargradient(
        spread:pad, 
        x1:0, y1:1, 
        x2:1, y2:0,
        stop:0 rgb(77, 132, 250),
        stop:1 rgb(79, 221, 240)
    );
	background: qlineargradient(
		spread:pad,
		x1:0, y1:1, 
		x2:1, y2:1,
		stop:0 rgba(242, 240, 254, 0),
		stop:1 rgba(107, 190, 207, 100)
	);
}







